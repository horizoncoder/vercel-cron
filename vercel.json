{
  "buildCommand": "pnpm turbo build",
  "ignoreCommand": "pnpm dlx turbo-ignore",
    "rewrites": [
        {"source": "/(.*)", "destination": "/"}
    ]
  "crons": [
    {
      "path": "/api/cron/1d",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/1w",
      "schedule": "0 0 * * 0"
    }
  ]
}
